<Window x:Class="PL.password"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="password" Height="350" Width="400">

    <Grid Margin="0,0,-13,-16">
        <Grid.Background>
            <ImageBrush ImageSource="\pic\hey2.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <TextBox HorizontalAlignment="Left" Margin="116,125,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <Button HorizontalAlignment="Left" Margin="10,10,0,320" Click="Button_Back" Width="35">
        </Button>
        <Label Content="put a password: " HorizontalAlignment="Left" Margin="136,31,0,0" VerticalAlignment="Top" Height="29" Width="100" Background="gray"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" Click="Button_Click" Height="30" Width="28"/>

    </Grid>

</Window>
