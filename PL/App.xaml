<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:LevelCollection x:Key="LevelCollection"/>
        <local:ConvertIdToContent x:Key="ConvertIdToContent"/>
        <local:ConvertNameToContent x:Key="ConvertNameToContent"/>
        <local:ConvertEmailToContent x:Key="ConvertEmailToContent"/>
        <local:ConvertSalaryToContent x:Key="ConvertSalaryToContent"/>
        <local:ConvertLevelToContent x:Key="ConvertLevelToContent"/>
        <local:ConvertTaskToContent x:Key="ConvertTaskToContent"/>
    </Application.Resources>
</Application>
